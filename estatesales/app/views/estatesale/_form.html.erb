<div id="estatesale-form">
  <%= form_for @sale do |f| %>
  <p>
      <%= f.label :title %> <br>
      <%= f.text_field :title %>
  </p>

  <p>
      <%= f.label :company_name %>
      <%= f.text_field :company_name %>
  </p>

  <p>
      <%= f.label :company_url %>
      <%= f.text_field :company_url %>
  </p>

  <p>
      <%= f.label :address %>
      <%= f.text_field :address %>
  </p>
  <p>
      <%= f.label :city %>
      <%= f.text_field :city %>
  </p>
  <p>
      <%= f.label :state %>
      <%= f.text_field :state %>
  </p>
  <p>
      <%= f.label :zip_code %>
      <%= f.text_field :zip_code %>
  </p>
  <p>
      <%= f.label :start_date %>
      <%= f.text_field :start_date %>
  </p>
  <p>
      <%= f.label :end_date %>
      <%= f.text_field :end_date %>
  </p>

  <p>
      <%= f.label :times %>
      <%= f.text_field :times %>
  </p>

  <p>
      <%= f.label :description %>
      <%= f.text_area :description %>
  </p>
  <p>
      <%= f.label :forms_of_payment %>
      <%= f.text_field :forms_of_payment %>
  </p>
  <p>
      <%= f.label :special_inst %>
      <%= f.text_field :special_inst %>
  </p>
  <p>
      <%= f.label :img_url %>
      <%= f.text_field :img_url %>
  </p>

  <br>
  <br>
  <p><%= f.submit %></p>
  <% end %>
</div>
